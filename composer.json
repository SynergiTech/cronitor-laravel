{
    "name": "synergitech/cronitor-laravel",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Josh Grant",
            "email": "josh@synergitech.co.uk"
        }
    ],
    "require": {
        "illuminate/support": "^9.0|^10.0|^11.0",
        "illuminate/contracts": "^9.0|^10.0|^11.0",
        "synergitech/cronitor": "^0.0.1"
    },
    "autoload": {
        "psr-4": {
            "SynergiTech\\Cronitor\\Laravel\\": "src/",
            "SynergiTech\\Cronitor\\Laravel\\Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "SynergiTech\\Cronitor\\Laravel\\CronitorServiceProvider"
            ]
        }
    },
    "config": {
        "allow-plugins": {
            "phpstan/extension-installer": true
        }
    },
    "require-dev": {
        "symplify/easy-coding-standard": "^9.2",
        "phpunit/phpunit": "^9.5|^10.0",
        "phpstan/extension-installer": "^1.1",
        "php-parallel-lint/php-parallel-lint": "^1.3",
        "larastan/larastan": "^2.9",
        "orchestra/testbench": "^7.0|^8.0|^9.0"
    }
}
